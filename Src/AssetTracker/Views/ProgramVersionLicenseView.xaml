<UserControl x:Class="AssetTracker.Views.ProgramVersionLicenseView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DockPanel LastChildFill="False" >
        <TextBlock DockPanel.Dock="Left"  x:Name="Key" />
        <StackPanel DockPanel.Dock="Right" Orientation="Horizontal">
            <Button x:Name="AssignUser" Content="Tildel" />
            <Button x:Name="EditLicense" Content="Endre" />
            <Button x:Name="DeleteLicense" Content="Slett" />
        </StackPanel>
    </DockPanel>
</UserControl>
